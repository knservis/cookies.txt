{
  "manifest_version": 2,
  "name": "cookie.txt",
  "version": "1.9",
  "description": "Exports your cookies super fast in a format compatible with wget",
  "short_name": "cookies",
  "offline_enabled": true,
  "permissions": [ "activeTab", "cookies", "tabs", "webNavigation", "http://*/*", "https://*/*", "contextMenus", "clipboardWrite"],
  "browser_action": {
    "default_title": "Export cookies.txt",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "page": "clipboard.html"
  }
}
